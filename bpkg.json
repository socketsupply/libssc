{
  "name": "libopc",
  "repo": "socketsupply/libopc",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Operator Framework client library written in C99.",
  "commands": {
    "print-version": "./scripts/version.sh",
    "indent": "indent -t2 -kr -nut src/**/*.c include/**/*.h",
    "lint": "cpplint  include/**/*.h src/**/*.c"
  },
  "files": [
    "include/buffer.h",
    "include/error.h",
    "include/init.h",
    "include/math.h",
    "include/opc.h",
    "include/platform.h",
    "include/result.h",
    "include/string.h",
    "include/uri.h",
    "include/utf8.h",
    "src/buffer.c",
    "src/error.c",
    "src/init.c",
    "src/log.c",
    "src/math.c",
    "src/result.c",
    "src/string.c",
    "src/system.c",
    "src/uri.c",
    "src/utf8.c"
  ],
  "scripts": [
    "scripts/build.sh",
    "scripts/version.sh"
  ],
  "dependencies": {
    "jwerle/brief.mk": "0.1.0"
  }
}

